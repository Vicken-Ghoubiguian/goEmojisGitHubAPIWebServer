{{define "main"}}

    {{template "header"}}

                <title>{{.Title}}</title>
            </head>
        <body class="main_body">
	   <p style="color: red;">Hello world !</p>
       <div>
            <table id="main_table">
                <thead>
                    <tr>
                        <th colspan="2" style="color: purple;">This is emojis table</th>
                    </tr>
                </thead>
                <tbody>
                    {{ range $key, $value := .ListOfEmojisFromGitHub }}
                        <tr class="main_table"><td>{{ $key }}</td><td><img src='{{ $value }}' alt='{{ $key }}' width='64' height='64'></td></tr>
                    {{ end }}
                </tbody>
            </table>
       </div>

        {{template "footer"}}

{{end}}
